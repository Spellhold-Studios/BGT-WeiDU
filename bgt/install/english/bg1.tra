@23816 = ~~ [WEREW01]
@23817 = ~~ [WEREW06]
@23818 = ~~ [WEREW05]
@23819 = ~~ [WEREW04]
@23820 = ~~ [WEREW03]
@23821 = ~~
@23822 = ~~ [GWERE03]
@23823 = ~~ [GWERE04]
@23824 = ~~ [GWERE05]
@23825 = ~~ [GWERE06]
@23826 = ~You're not figments, are you? I'm not wasting any more time talking to figments!~ [DRADE_01]
